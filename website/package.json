{
  "name": "website",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "npm run test:integration && npm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "generate:client": "openapi-generator generate -i http://127.0.0.1:5001/agents-as-a-service/us-central1/api/openapi.json -g typescript -o .generated/api-client --additional-properties=npmName=api-client,supportsES6=true,npmVersion=6.9.0,withInterfaces=true && npm i",
    "generate:client:proxy": "openapi-generator generate -i http://localhost:5002/api/openapi.json -g typescript -o .generated/api-client --additional-properties=npmName=api-client,supportsES6=true,npmVersion=6.9.0,withInterfaces=true && npm i",
    "firebase:emulator": "firebase emulators:start",
    "firebase:deploy": "firebase deploy",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
