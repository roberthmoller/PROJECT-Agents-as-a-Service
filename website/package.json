{
  "name": "website",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "npm run test:integration && npm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "generate:client:url": "openapi-generator-cli generate -i http://localhost:5002/api/openapi.json -g typescript -o .generated/api-client --additional-properties=npmName=restClient,supportsES6=true,npmVersion=6.9.0,withInterfaces=true && npm i",
    "generate:client:file": "openapi-generator-cli generate -i openapi.json -g typescript -o .generated/api-client --additional-properties=npmName=restClient,supportsES6=true,npmVersion=6.9.0,withInterfaces=true && npm i",
    "firebase:serve": "firebase serve"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.9.0",
    "@playwright/test": "^1.28.1",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "api-client": "file:.generated/api-client",
    "autoprefixer": "^10.4.16",
    "lucide-svelte": "^0.321.0",
    "postcss": "^8.4.32",
    "postcss-load-config": "^5.0.2",
    "sass": "^1.70.0",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "svelte-routing": "^2.11.0",
    "tailwindcss": "^3.3.6",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.3",
    "vitest": "^1.2.0"
  },
  "overrides": {
    "rollup": {
      "@rollup/rollup-darwin-x64": {
        "@rollup/rollup-darwin-arm64": "4.9.6"
      }
    }
  },
  "dependencies": {
    "@openapitools/openapi-generator-cli": "^2.9.0",
    "bits-ui": "^0.16.0",
    "clsx": "^2.1.0",
    "cmdk-sv": "^0.0.13",
    "firebase": "^10.8.0",
    "mode-watcher": "^0.2.0",
    "radix-icons-svelte": "^1.2.1",
    "restClient": "file:.generated/api-client",
    "rxfire": "^6.0.5",
    "rxjs": "^7.8.1",
    "tailwind-merge": "^2.2.1",
    "tailwind-variants": "^0.1.20"
  }
}
